package round2;

public class SearchInSortedMatrix {
	public int[] search(int[][] matrix, int target) {
	    // Write your solution here.
	    if (matrix.length == 0 || matrix[0].length == 0) {
	      return new int[] {-1, -1};
	    }
	    int rows = matrix.length;
	    int cols = matrix[0].length;
	    return new int[] {-1,-1};
}
