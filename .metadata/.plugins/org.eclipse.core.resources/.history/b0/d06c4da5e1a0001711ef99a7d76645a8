package round3;

public class CheckCycle {
	public boolean checkCycle (ListNode head) {
		ListNode slow = head;
		ListNode fast = head;
		while (fast.next!=null&&fast.next.next!=null) {
			slow=slow.next;
			fast = fast.next.next;
			if (fast == slow) {
				return true;
			}
		}
		return false;
	}
	
}
